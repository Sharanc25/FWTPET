journal = {Materials and Design},
pages = {556--564},
publisher = {Elsevier Ltd},
title = {{Analysis of heat input effect on the mechanical properties of Al-6061-T6 alloy weld joints}},
url = {http://dx.doi.org/10.1016/j.matdes.2013.05.081},
volume = {52},
year = {2013}
